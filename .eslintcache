ll>\n                                            <TableCell align=\"right\">P.U.</TableCell>\n                                            <TableCell align=\"right\">Importe</TableCell>\n                                            <TableCell align=\"right\">Opciones</TableCell>\n                                        </TableRow>\n                                        </TableHead>\n                                        <TableBody>\n                                        {/* Here Goes Some Content */}\n                                        </TableBody>\n                                    </Table>\n                                    </TableContainer><br/>\n                                    <Button style=\n                                                {{ backgroundColor: 'white' }}\n                                                onClick={() => showAllSales()}\n                                    >Regresar</Button>\n                                    </>                 \n                            </div>\n                            </div>\n\n                        : <SpacingGrid />\n                        }\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Pedidos)\n\n","/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/components/ComponentA.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/pages/MyContext.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/components/ComponentB.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/components/ComponentC.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/components/userContext.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/utils/formatDecimals.js",[],"/media/edgar/DATA/eCommerce/deployable/vegetables-ecommerce/src/pages/Verification.js",[],"/media/edgar/DATA/eCommerce/deploya